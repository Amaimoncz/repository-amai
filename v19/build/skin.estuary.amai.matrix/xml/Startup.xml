<?xml version="1.0" encoding="UTF-8"?>
<window>
	<onload>ReplaceWindow($INFO[System.StartupWindow])</onload>
	<controls />
	<!-- Set default values if SCC_movies widget is OFF -->
	<onload condition="!Skin.HasSetting(SCCMainMenuWidget_Movies)">Skin.SetBool(SCC_movies_none1)</onload>
	<onload condition="!Skin.HasSetting(SCCMainMenuWidget_Movies)">Skin.SetBool(SCC_movies_none2)</onload>
	<onload condition="!Skin.HasSetting(SCCMainMenuWidget_Movies)">Skin.SetBool(SCC_movies_none3)</onload>
